  <script>
    function goRoom(url) {
      window.location = url
    }
    new WOW({ animateClass: 'animated', boxClass: 'wow' }).init()
    function goRoom(type) {
      window.location = type.length > 2 ? type : './room.html?type=' + type

    }
    var mySwiper = new Swiper('.swiper', {
      loop: true,
      autoplay: true,
      pagination: {
        el: '.swiper-pagination',
        clickable: true,
        renderBullet: function (index, className) {
          let texts = ['骏马食品工厂', '骏马房地产', '骏马酒店']
          let texten = ['Junma Real Estate', 'Junma Food Factory', 'Junma Hotel']
          return `
                    <div  class="${className} swiperItem">
                        <div class='bullet'></div>
                        <div class="swiper-pagination-text">
                            <p class='text-2xl'>${texts[index]}<p>
                            <p>${texten[index]}<p>
                                </div>
                    </div>
                    `;
        },
      },
    })
  </script>
